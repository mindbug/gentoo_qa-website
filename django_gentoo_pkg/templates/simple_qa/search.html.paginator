{% extends "simple_qa/base.html" %}
{% load pagination_tags %}

{% block content %}
<form action="." method="post">
    {% csrf_token %}
    {{ form.as_p }}
    <input type="submit" value="Submit" />
</form>

<p>{{ result_message }}</p>

{% autopaginate results %}
{% for result in results %}
    <p>{{ result.package }}</p>
{% endfor %}
{% paginate %}
{% endblock %}
